package Exercise_5_24;

public class DiamondPrinting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				if (i <= 5 && j <= 5) {
					int a =1;
					if (a<=5-i)
						System.out.print(" ");
					}
					for (int b = 1; b <= i; b++) {
						System.out.print("*");
					}
				} else if (i <= 5 && j > 5) {

				}

			}
			System.out.println();
		}

	}

}
