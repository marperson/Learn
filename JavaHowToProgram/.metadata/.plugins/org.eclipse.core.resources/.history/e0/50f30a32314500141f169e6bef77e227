package Exercise_5_20;

public class Pi {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double pi=0;
		int accu=1;
		while (accu<20000){
			if (accu%2==0){
				pi-=4/(2*accu-1);
				}
			else if(accu%2!=0){
				pi+=4/(2*accu-1);
			}
			accu++;
		}
		System.out.println(pi);

	}

}
